<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/home.css">
    <script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.11"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/split.js/1.6.0/split.min.js"></script>
    <title>Document</title>
    
</head>
<body class="center" onload="preloader();">
    
    <h1 id="type"></h1>
    
    <script>
    var options = {
    strings: ['Loading'],
    typeSpeed: 40,
    backSpeed:100,
    loop: true ,
    loopCount: Infinity
    };

    var typed = new Typed('#type', options);
    </script>

    <!-- <h1>Page loaded</h1> -->

    <script>
        function preloader() {
            let delay = setTimeout(()=>{
                let type = document.getElementsByClassName('typed-cursor')[0] ;
                type.parentNode.removeChild(type);
                let h1Div = document.getElementById('type');
                h1Div.parentNode.removeChild(h1Div);
                let removeHidden = document.getElementsByClassName('hidden')[0];
                removeHidden.classList.toggle('hidden');
                document.body.classList.toggle('center');
            },3000);
            
        }
    </script>
    <div class="hidden main">
        <div class="a"></div>
        <div class="b"></div>
    </div>
    <script src="/js/home.js" type="text/javascript"></script>
</body>
</html>